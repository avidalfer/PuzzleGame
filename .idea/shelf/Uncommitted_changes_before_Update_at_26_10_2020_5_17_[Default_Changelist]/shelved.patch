+package com.example.puzzlegame.models;
+
+import java.util.ArrayList;
+import java.util.List;
+
+public class User {
+
+    private Level userLvl;
+    private final List<GameSession> playedGames;
+    private int currentSession;
+
+    public User() {
+        playedGames = new ArrayList<>();
+    }
+
+    public Level getUserLvl() {
+        return userLvl;
+    }
+
+    public void setUserLvl(Level lvl) {
+        this.userLvl = lvl;
+    }
+
+    public GameSession getLastSession() {
+        int lastGame = playedGames.size() - 1;
+        return playedGames.get(lastGame);
+    }
+
+    public void addPlayedGame(GameSession session) {
+        this.playedGames.add(session);
+    }
+
+    public GameSession getCurrentSession(Integer sessionId) {
+        return playedGames.get(sessionId);
+    }
+
+    public void setCurrentSession (int sessionId) {
+        currentSession = sessionId;
+    }
+}
